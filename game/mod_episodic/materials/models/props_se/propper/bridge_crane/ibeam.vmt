"vertexlitgeneric"
{
	   "$basetexture"	"se_metal\ibeam"

	 // suraface type
	    $surfaceprop "metal"
 
	 // add envcube
	    $envmap env_cubemap
	    $basealphaenvmapmask "1"
	    $envmaptint "[.05 .09 .1]" 
	    $envmapcontrast 1
	    $envmapsaturation .5

	"$detail" "detail\metal_detail_01"
	"$detailscale" "8.0"
	"$detailblendfactor" .8
	"$detailblendmode" "0"
}

